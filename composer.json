{
    "name": "phlexible/indexer-page-bundle",
    "description": "phlexible.cms indexer page bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["indexer", "search", "content"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\IndexerPageBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "phlexible/element-bundle": "~1.2",
        "phlexible/indexer-bundle": "~1.2",
        "phlexible/queue-bundle": "~1.2",
        "phlexible/siteroot-bundle": "~1.2",
        "phlexible/tree-bundle": "~1.2",
        "psr/log": "^1.0",
        "symfony/dom-crawler": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "symfony/css-selector": "^2.8"
    },
    "suggest": {
        "phlexible/frontend-search-bundle": "Default frontend search for elements"
    },
    "replace": {
        "phlexible/indexer-element-bundle": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
