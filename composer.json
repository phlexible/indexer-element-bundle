{
    "name": "phlexible/indexer-element-bundle",
    "description": "phlexible.cms indexer element bundle",
    "license": "proprietary",
	"type": "symfony-bundle",
	"keywords": ["indexer", "search"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\IndexerElementBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.3.0",
        "phlexible/indexer-bundle": "~1.0",
        "psr/log": "~1.0",
        "symfony/dom-crawler": "~1.0"
    },
    "suggest": {
        "phlexible/frontend-search-bundle": "Default frontend search for elements"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.brainbits.net/phlexible-bundles/",
            "options": {
                "ssl": {
                    "allow_self_signed": "true"
                }
            }
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
