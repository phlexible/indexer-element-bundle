{
    "name": "phlexible/indexer-page-bundle",
    "description": "phlexible indexer page bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["indexer", "search", "content"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\IndexerPageBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "phlexible/element-bundle": "^1.3",
        "phlexible/element-renderer-bundle": "^1.3",
        "phlexible/indexer-bundle": "^1.1.3",
        "phlexible/queue-bundle": "^1.3",
        "phlexible/siteroot-bundle": "^1.3",
        "phlexible/tree-bundle": "^1.3",
        "psr/log": "^1.0",
        "symfony/dom-crawler": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "symfony/css-selector": "^2.8",
        "symfony/config": "^2.8",
        "symfony/dependency-injection": "^2.8",
        "symfony/http-foundation": "^2.8",
        "symfony/routing": "^2.8",
        "symfony/templating": "^2.8"
    },
    "suggest": {
        "phlexible/frontend-search-bundle": "Default frontend search for elements"
    },
    "replace": {
        "phlexible/indexer-element-bundle": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
